{
	"name": "labelling",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host=labelling.localhost --port=8081",
		"preview-wrangler": "npx wrangler pages dev ./.svelte-kit/cloudflare --compatibility-date=2023-07-24",
		"build": "vite build",
		"preview": "vite preview --host=labelling.localhost --port=8080",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "2.3.1",
		"@sveltejs/kit": "1.22.3",
		"@typescript-eslint/eslint-plugin": "6.2.0",
		"@typescript-eslint/parser": "6.2.0",
		"autoprefixer": "10.4.14",
		"eslint": "8.46.0",
		"eslint-config-prettier": "8.9.0",
		"eslint-plugin-svelte": "2.32.4",
		"postcss": "8.4.27",
		"prettier": "3.0.0",
		"prettier-plugin-svelte": "3.0.3",
		"prettier-plugin-tailwindcss": "0.4.1",
		"svelte": "4.2.0",
		"svelte-check": "3.4.6",
		"tailwindcss": "3.3.3",
		"tslib": "2.4.1",
		"typescript": "5.1.6",
		"vite": "4.4.9",
		"atreyu": "file:../cloudless/atreyu"
	},
	"type": "module"
}
