{
  "name": "labelling",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port=8081",
    "preview-wrangler": "npx wrangler pages dev ./.svelte-kit/cloudflare --compatibility-date=2023-07-24",
    "build": "vite build",
    "preview": "vite preview --host=labelling.localhost --port=8080",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "2.3.1",
    "@sveltejs/kit": "1.24.1",
    "@typescript-eslint/eslint-plugin": "6.2.0",
    "@typescript-eslint/parser": "6.2.0",
    "atreyu": "0.9.54",
    "autoprefixer": "10.4.14",
    "eslint": "8.46.0",
    "eslint-config-prettier": "8.9.0",
    "eslint-plugin-svelte": "2.33.1",
    "postcss": "8.4.27",
    "prettier": "3.0.3",
    "prettier-plugin-svelte": "3.0.3",
    "prettier-plugin-tailwindcss": "0.4.1",
    "svelte": "4.2.0",
    "svelte-check": "3.5.1",
    "tailwindcss": "3.3.3",
    "tslib": "2.6.2",
    "typescript": "5.2.2",
    "vite": "4.4.9"
  }
}
